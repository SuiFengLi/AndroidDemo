<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.androidy.azsecuer.activity.PhoneRocketActivity">

    <include layout="@layout/include_home_actionbar"/>
    <TextView
        android:id="@+id/tv_phone_rocket_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Title"
        android:textSize="30sp"
        android:gravity="center"/>
    <com.androidy.azsecuer.view.BatteryProgressBarMove
        android:id="@+id/pb_phone_rocket"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_height="15dp"
        android:progressDrawable="@drawable/pb_memory_bg"
        android:max="100"
        android:progress="50"/>
    <TextView
        android:id="@+id/tv_phone_rocket_memory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="sss"
        android:gravity="right"
        android:layout_marginRight="10dp"/>

    <Button
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_phone_rocket_show"
        android:text="@string/showSystem"
        android:id="@+id/btn_phone_rocket_show" />
    <include layout="@layout/include_framelayout_listview"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="10dp">
        <CheckBox
            android:id="@+id/cb_phone_rocket_select_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Button
            android:id="@+id/btn_phone_rocket_delete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/aKeyClean"/>


    </LinearLayout>

</LinearLayout>
